{
  "name": "landing",
  "version": "1.0.0",
  "description": "retournelamphi landing page",
  "main": "server/index.js",
  "scripts": {
    "build": "npm run clean && babel --optional runtime server -d dist/server && npm run cp-files && npm run webpack",
    "clean": "rm -rf dist",
    "cp-files": "cp package.json dist/package.json && cp -rf server/views dist/server/views",
    "webpack": " webpack --bail --progress --profile",
    "start": "node server/index.js",
    "babel-nodemon": "nodemon --exec ./node_modules/.bin/babel-node --",
    "start-dev": "NODE_ENV=development DEBUG=express:* npm run babel-nodemon server/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/retournelamphi/landing.git"
  },
  "keywords": [
    "node",
    "angular",
    "mongo"
  ],
  "author": "Jermorin Llaine",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/retournelamphi/landing/issues"
  },
  "homepage": "https://github.com/retournelamphi/landing#readme",
  "dependencies": {
    "body-parser": "^1.15.0",
    "compression": "^1.6.1",
    "cookie-parser": "^1.4.1",
    "cors": "^2.7.1",
    "debug": "^2.2.0",
    "ejs": "^2.4.1",
    "express": "^4.13.4",
    "express-validator": "^2.10.0",
    "method-override": "^2.3.5",
    "mongoose": "^4.4.3",
    "morgan": "^1.6.1",
    "request-ip": "^1.2.2",
    "sendgrid": "^2.0.0"
  },
  "devDependencies": {
    "null-loader": "^0.1.1",
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.5.1",
    "file-loader":"^0.9.0",
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.0",
    "url-loader":"^0.5.7",
    "ng-annotate-webpack-plugin":"^0.1.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "nodemon": "^1.8.1",
    "postcss-loader": "^0.8.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13"
  }
}
